<div id="<%= dom_id food_nutrient %>">
  <p>
    <strong>Id:</strong>
    <%= food_nutrient.id %>
  </p>

  <p>
    <strong>Nutrient:</strong>
    <%= food_nutrient.nutrient_id %>
  </p>

  <p>
    <strong>Food:</strong>
    <%= food_nutrient.food_id %>
  </p>

  <p>
    <strong>Study:</strong>
    <%= food_nutrient.study %>
  </p>

  <p>
    <strong>Study weight:</strong>
    <%= food_nutrient.study_weight %>
  </p>

  <p>
    <strong>Avg rec:</strong>
    <%= food_nutrient.avg_rec_id %>
  </p>

  <p>
    <strong>Portion:</strong>
    <%= food_nutrient.portion %>
  </p>

  <p>
    <strong>Portion unit:</strong>
    <%= food_nutrient.portion_unit %>
  </p>

  <p>
    <strong>Amount:</strong>
    <%= food_nutrient.amount %>
  </p>

  <p>
    <strong>Amount unit:</strong>
    <%= food_nutrient.amount_unit %>
  </p>

  <p>
    <strong>Desc:</strong>
    <%= food_nutrient.desc %>
  </p>

</div>
