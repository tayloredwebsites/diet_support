<%
  @title = "Nutrients Listing"
  content_for(:title, @title)
%>
<p style="color: green"><%= notice %></p>

<h1><%= @title %></h1>

<div id="nutrients">
  <table>
    <%= render 'nutrient_headers' %>
    <% @nutrients.each do |nutrient| %>
      <%= render nutrient %>
    <% end %>
  </table>
</div>

<ul>
<li><%= link_to "New nutrient", new_nutrient_path %></li>
<li><a href='/foods'>Foods Listing</a></li>
<li><a href="/">Home</a></li>
<li><a href='/signout'>Sign Out</a></li>
</ul>
