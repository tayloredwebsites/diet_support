<div id="food_id_<%= @food.id %>">
  <p>
    <strong>Id:</strong>
    <%= @food.id %>
  </p>
  <p>
    <strong>Name:</strong>
    <%= @food.name %>
  </p>
  <p>
    <strong>Portion:</strong>
    <%= @food.food_portion_amount %>
    <%= @food.food_portion_unit %>
    - <%= LookupTable::VALID_UNIT_CODES[@food.food_portion_unit] %>

  </p>
  <p>
    <strong>USDA food category ID:</strong>
    <%= @food.usda_food_cat_id %>
    - <%= (@food.usda_food_cat.present?) ? @food.usda_food_cat.lu_desc : "" %>
  </p>
  <p>
    <strong>WWEIA food category ID:</strong>
    <%= @food.wweia_food_cat_id %>
    - <%= (@food.wweia_food_cat_id.present?) ? @food.wweia_food_cat.lu_desc : "" %>
  </p>
  <p>
    <strong>USDA FDC IDs:</strong>
    <%= @food.usda_fdc_ids_json %>
  </p>
</div>
